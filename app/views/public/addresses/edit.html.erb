<h1>Public::Addresses#edit</h1>
<p>Find me in app/views/public/addresses/edit.html.erb</p>

  <%= form_with model: @address,url:address_path, method: :patch, local:true do |f| %>

    <h4>宛名</h4>
    <%= f.text_field :name %>

    <h4>郵便番号</h4>
    <%= f.text_field :postal_code %><br />

    <h4>住所</h4>
    <%= f.text_field :address %><br />

    <%= f.submit '編集内容を保存' %>
  <% end %>